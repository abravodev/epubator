<?xml version="1.0" encoding="utf-8" ?>
<resources>
    <style name="AppTheme" parent="@style/Theme.AppCompat.Light">
        <item name="actionBarStyle">@style/ActionBarStyle</item>
        <item name="actionOverflowButtonStyle">@style/ActionBar.OverflowButtonStyle</item>
        <item name="android:actionMenuTextColor">@android:color/white</item>
    </style>

    <style name="AppTheme.MainView" parent="@style/AppTheme">
        <item name="actionBarStyle">@style/ActionBarStyle.NoTitle</item>
        <item name="actionOverflowButtonStyle">@style/ActionBar.NoTitle.OverflowButtonStyle</item>
        <item name="android:actionMenuTextColor">@color/dark_green</item>
    </style>
</resources>
